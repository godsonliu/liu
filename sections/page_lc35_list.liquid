<style>
@media(min-width: 767px) {
  .showMobile {
    display: none;
  }
}
@media(max-width: 767px) {
  .showPc {
    display: none;
  }
}
.page_lc35_list {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 20px 100px 20px;
}
.page_lc35_list .lc35_list_left {
  position: relative;
  font-size: 0;
  margin-right: 10px;
}
.page_lc35_list .lc35-mask {
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.3);
  left: 0;
  top: 0;
  position: absolute;
  z-index: 1;
  border-radius: 35px;
}
.page_lc35_list .watch_video {
  width: 100%;
  position: absolute;
  left: 50%;
  top: 80px;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 700;
  font-size: 30px;
  color: #fff;
  z-index: 2;
  cursor: pointer;
}
.page_lc35_list .watch_video svg {
  margin-right: 10px;
  width: 30px;
  height: 30px;
}
.page_lc35_list .lc35_img {
  border-radius: 35px;
}
.lc35_list_right_top {
  margin-bottom: 10px;
}
.lc35_list_right_top, .lc35_list_right_bottom {
  position: relative;
  font-size: 0;
}
.page_lc35_list .lc35_title {
  position: absolute;
  left: 30px;
  top: 30px;
  color: #404040;
  font-size: 30px;
  line-height: 30px;
  font-weight: 700;
}
@media(max-width: 767px) {
  .page_lc35_list {
    padding: 0 37.5px;
    flex-direction: column;
  }
  .page_lc35_list .mobile-inline {
    display: inline-block;
  }
  .page_lc35_list .watch_video {
    top: 38.5px;
    font-size: 18px;
  }
  .page_lc35_list .watch_video svg {
    width: 18px;
    height: 18px;
  }
  .page_lc35_list .lc35_list_left {
    margin: 0 0 25px 0;
  }
  .page_lc35_list .lc35_list_left, .page_lc35_list .lc35_list_right {
    width: 100%;
  }
  .page_lc35_list .lc35_list_right .lc35_img {
    width: 100%;
  }
  .page_lc35_list .lc35_list_right_top, .page_lc35_list .lc35_list_right_bottom {
    width: 100%;
    margin-bottom: 25px;
  }
  .page_lc35_list .lc35_title {
    width: 100%;
    text-align: center;
    left: 0;
    font-size: 24px;
  }
}
</style>

<div class="page_lc35_list" id="overview">
  <div class="lc35_list_left">
    <div class="lc35-mask"></div>
    <div class="watch_video">
      <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <rect width="20" height="20" fill="url(#pattern0)"/>
        <defs>
        <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
        <use xlink:href="#image0_29_16" transform="scale(0.0125)"/>
        </pattern>
        <image id="image0_29_16" width="80" height="80" xlink:href="data:image/png;base64,"/>
        </defs>
      </svg>
      {{ section.settings.watch_video }}
    </div>
    <img class="showPc lc35_img lazyload" data-src="{{ section.settings.left_image | image_url }}" width="750" height="665" />
    <img class="showMobile lc35_img lazyload" data-src="{{ section.settings.mobile_left_image | image_url }}" width="1200" height="1400" />
  </div>
  <div class="lc35_list_right">
    <div class="lc35_list_right_top">
      <div class="lc35_title">{{ section.settings.right_top_title }}</div>
      <img class="showPc lc35_img lazyload" data-src="{{ section.settings.right_top_image | image_url }}" width="540" height="327" />
      <img class="showMobile lc35_img lazyload" data-src="{{ section.settings.mobile_right_top_image | image_url }}" width="1200" height="1400" />
    </div>
    <div class="lc35_list_right_bottom">
      <div class="lc35_title">{{ section.settings.right_bottom_title }}</div>
      <img class="showPc lc35_img lazyload" data-src="{{ section.settings.right_bottom_image | image_url }}" width="540" height="327" />
      <img class="showMobile lc35_img lazyload" data-src="{{ section.settings.mobile_right_bottom_image | image_url }}" width="1200" height="1400" />
    </div>
  </div>
</div>

{% render 'component_video_player_popup', div_id: section.id %}

<script>
$(function() {
  const video_url = '{{ section.settings.video_url }}'
	const mobile_video_url = '{{ section.settings.mobile_video_url }}'
	const deviceWidth = document.body.clientWidth
	$('.page_lc35_list .watch_video').on('click', function() {
		if(window['video_player_popup_{{section.id}}']) {
      dataLayer.push({
        "event": "uaEvent", 
        "eventCategory": "Activity Page_{{ section.settings.ga_handle }}",
        "eventAction": "watch_video",
        "eventLabel": "{{ section.settings.video_title }}", //取视频title
      })
      window.dataLayer.push({ "event_parameters": null });
      window.dataLayer.push({
        "event": "ga4Event",
        "event_name": "video_start",
        "event_parameters": {
          "video_title": "{{ section.settings.video_title }}", 
          "page_group": "Activity Page_{{ section.settings.ga_handle }}"
        }
      })
			if(deviceWidth > 767) {
				video_url && window['video_player_popup_{{section.id}}'].show(video_url)
			}else {
				mobile_video_url && window['video_player_popup_{{section.id}}'].show(mobile_video_url)
			}
		}
	})
})
</script>

