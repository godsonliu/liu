{% if shop.metafields['global']['common'].value %}
  {% assign info_select_policy = shop.metafields['global']['common'].value %}
{% else %}
  {% assign info_select_policy = shop.metafields['global']['common'] %}
{% endif %}

<div class="hidden">
{% assign id = section.id %}
{% assign settings = section.settings %}
</div>

{{ 'LiveStreaming.scss.css' | asset_url | stylesheet_tag }}

{% style %}
  
#LiveStreaming_{{id}} {
  background: {{settings.box_background}};
}
#LiveStreaming_{{id}} .box_title {
  font-size: {{ settings.title_font_size}};
  color: {{ settings.title_color}};
  font-weight: {{settings.title_font_weight}};
}
{% if settings.box_text_color %}
  #LiveStreaming_{{id}} .actionsList {
    color: {{ settings.box_text_color}};
  }
{% endif %}
@media(max-width: 769px) {
  #LiveStreaming_{{id}} .box_title {
    font-size: {{ settings.mobile_title_font_size}};
  }
}

{% if settings.box_padding %}
  #LiveStreaming_{{id}} .comingSoonContainer {
    padding: {{ settings.box_padding}};
  }
{% endif %}
{% if settings.mobile_box_padding %}
@media(max-width: 769px) {
  #LiveStreaming_{{id}} .comingSoonContainer {
    padding: {{ settings.mobile_box_padding}};
  }
}
{% endif %}

{% endstyle %}

<div id="LiveStreaming_{{ id }}" class="LiveStreaming">
  {% if settings.youtube_iframe != '' %}
  <div class="model__container flexBox">
    <div class="liveBox">
      {{ settings.youtube_iframe }}
    </div>
    {% if settings.youtube_chat != '' %}
    <div class="chatBox">
      <iframe src="https://www.youtube.com/live_chat?{{ settings.youtube_chat }}" title="youtube chat"></iframe>
