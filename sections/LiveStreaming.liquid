<script>
  $(document).on('shopify:section:load', function(e){

  })

  
  $(document).ready(function() {
    let timer = null;
    let timeLine = 0;
    let isMoving = false;
    let totalBrands = $("#LiveStreaming_{{ id }} .flex .line").length;

    const isMobile = window.innerWidth < 769;
    if (totalBrands <= 3) {
      $('#LiveStreaming_{{ id }} .front').addClass('hidden');
      $('#LiveStreaming_{{ id }} .back').addClass('hidden');
    }
    $("#LiveStreaming_{{ id }} .line:nth-child(4)").addClass('opacity0');
    function comingSoonAct(num) {
      if (timeLine === 0 && num < 0) return false;
      if ((timeLine + 3) >= totalBrands && num > 0) return false;
      if (isMoving) return false;
      $('#LiveStreaming_{{ id }} .flex').removeClass(`move${timeLine}`)
      isMoving = true;
      timeLine += num;
      $('#LiveStreaming_{{ id }} .flex').addClass(`move${timeLine}`)
      $('#LiveStreaming_{{ id }} .line').removeClass('opacity0')
      $(`#LiveStreaming_{{ id }} .line[data-line="${timeLine}"]`).addClass('opacity0')
      $(`#LiveStreaming_{{ id }} .line[data-line="${timeLine + 4}"]`).addClass('opacity0')
      window.setTimeout(function() {isMoving = false}, 300)
    }

    $('#LiveStreaming_{{ id }}').on('click', '.front', function(e) {
      comingSoonAct(-1)
    });

    $('#LiveStreaming_{{ id }}').on('click', '.back', function(e) {
      comingSoonAct(1)
    });

    $('#LiveStreaming_{{ id }}').on('click', '.brandname button', function(e) {
      console.log('brand', isMobile)
      if (isMobile) {
        const brand = $(this).data('brand');
        $('.popup_{{ id }} .brand .iconfont').addClass('hidden')
        $('.popup_{{ id }} .actionsPopup').addClass('hidden')
        $(`.popup_{{ id }} .container .${brand}`).removeClass('hidden')
        $('.popup_{{ id }}').removeClass('hidden')
      }
      return false
    });

    $('.popup_{{ id }}').on('click', '.xIcon', function(e) {
      if (isMobile) {
        $('.popup_{{ id }}').addClass('hidden')
      }
    });

    $('.popup_agree_{{ id }}').on('click', '.submit button', function(e) {
      const policy =  $('.popup_agree_{{ id }} input[name="policy"]').prop("checked");
      console.log('policy', policy);
      if (!policy) {
        $(`.popup_agree_{{ id }} .form__error`).removeClass('visible-none');
        if (timer) clearTimeout(subscribe_private.timer);
        timer = setTimeout(() => { 
          $(`.popup_agree_{{ id }} .form__error`).addClass('visible-none');
        }, 8000);
      } else {
        $('.popup_agree_{{ id }}').addClass('hidden');
          Cookies.set("LIVE_lottery_policy", 'Agree',{
            domain: window.location.host,
            expires: 365,
          })
      }
    });

    const lottery_policy = Cookies.get('LIVE_lottery_policy');
    const show_agree_popup = '{{ section.settings.show_agree }}';
    console.log('lottery_policy', lottery_policy, show_agree_popup)

    if (lottery_policy !== 'Agree' && show_agree_popup === 'true') {
      $('.popup_agree_{{ id }}').removeClass('hidden');
    }
  })
</script>

