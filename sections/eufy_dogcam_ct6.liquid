{% assign lang = product.metafields.global %}

{% if lang.dogcam.value %}
  {% assign dogcam = lang.dogcam.value %}
{% else %}
  {% assign dogcam = lang.dogcam %}
{% endif %}

<link rel="stylesheet" href="{{ 'eufy_dogcam_ct.scss.css' | asset_url }}">
<script src="{{ 'eufy_dogcam_ct.js' | asset_url }}" defer="defer"></script>


<div class="eufy_dogcam_6">
  <div class="banner_6">
    <div class="banner_5_c">
      <div class="banner_5_c_left">
        <div class="banner_5_c_left_content">
          <div class="banner_5_img4">
            <img class="pc_show lazyload" data-src="{{ dogcam.banner_5_img4 }}" alt="">
            <img class="mob_show lazyload" data-src="{{ dogcam.banner_5_img4_mob }}" alt="">
          </div>
          <div class="banner_5_img4_bg pc_show">
            <div class="banner_5_img4_p"><p>{{ dogcam.banner_5_img4_p }}</p></div>
          </div>
        </div>
        <div class="banner_5_c_left_p">{{ dogcam.banner_5_c_left_p }}</div>
        <div class="banner_5_img4_bg mob_show">
          <div class="banner_5_img4_p"><p>{{ dogcam.banner_5_img4_p }}</p></div>
        </div>
      </div>
      <div class="banner_5_c_right">
        <div class="banner_5_img5_g pc_show">
          <img class="lazyload" data-src="{{ dogcam.banner_5_img5_g }}" alt="">
        </div>
        <div class="banner_5_c_right_content">
          <div class="banner_5_img5">
            <img class="pc_show lazyload" data-src="{{ dogcam.banner_5_img5 }}" alt="">
            <img class="mob_show lazyload" data-src="{{ dogcam.banner_5_img5_mob }}" alt="">
          </div>
          <div class="banner_5_img5_bg pc_show">
            <div class="banner_5_img5_p"><p>{{ dogcam.banner_5_img5_p }}</p></div>
          </div>
        </div>
        <div class="banner_5_c_right_p">{{ dogcam.banner_5_c_right_p }}</div>
        <div class="banner_5_img5_bg mob_show">
          <div class="banner_5_img5_p"><p>{{ dogcam.banner_5_img5_p }}</p></div>
        </div>
      </div>
    </div>
  </div>
</div>