  $(document).ready(function(){
    function setScrollTop() {
      
      const screenHeight = window.innerHeight - 100;
      $('#unlock').css("height", `${screenHeight * 6}px`);
      $('#unlock .sticky_container').css("height", `${screenHeight}px`);
      const unlockTop = $('#unlock')[0].getBoundingClientRect().top
      const unlockScrolled = - (unlockTop / (screenHeight * 5));

      $('#unlock .imgBox').find('.img').css('opacity', '0');
      $('#unlock .txtBox').find('.txt').css('opacity', '0.2');
      if (unlockScrolled >= 0.2) {
        $('#unlock .imgBox .img1').css('opacity', '0');
        $('#unlock .txtBox .txt1').css('opacity', '0.2');
      } else {
        $('#unlock .imgBox .img1').css('opacity', '1');
        $('#unlock .txtBox .txt1').css('opacity', '1');
      }
      if (unlockScrolled >= 0 && unlockScrolled < 0.2) {
        $('#unlock .txtArr').css('transform', 'translateY(0%)');
      } else if (unlockScrolled >= 0.2 && unlockScrolled < 0.35) {
        $('#unlock .txtArr').css('transform', 'translateY(-20%)');
        $('#unlock .imgBox .img2').css('opacity', '1');
        $('#unlock .txtBox .txt2').css('opacity', '1');
      } else if (unlockScrolled >= 0.35 && unlockScrolled < 0.5) {
        $('#unlock .txtArr').css('transform', 'translateY(-40%)');
        $('#unlock .imgBox .img3').css('opacity', '1');
        $('#unlock .txtBox .txt1').css('opacity', '0');
        $('#unlock .txtBox .txt3').css('opacity', '1');
      } else if (unlockScrolled >= 0.5 && unlockScrolled < 0.65) {
        $('#unlock .txtArr').css('transform', 'translateY(-60%)');
        $('#unlock .imgBox .img4').css('opacity', '1');
        $('#unlock .txtBox .txt1').css('opacity', '0');
        $('#unlock .txtBox .txt2').css('opacity', '0');
        $('#unlock .txtBox .txt4').css('opacity', '1');
      } else if (unlockScrolled >= 0.65 && unlockScrolled < 0.8) {
        $('#unlock .txtArr').css('transform', 'translateY(-75%)');
        $('#unlock .imgBox .img5').css('opacity', '1');
        $('#unlock .txtBox .txt1').css('opacity', '0');
        $('#unlock .txtBox .txt2').css('opacity', '0');
        $('#unlock .txtBox .txt3').css('opacity', '0');
        $('#unlock .txtBox .txt5').css('opacity', '1');
      } else if (unlockScrolled >= 0.8) {
        $('#unlock .txtArr').css('transform', 'translateY(-90%)');
        $('#unlock .imgBox .img6').css('opacity', '1');
        $('#unlock .txtBox .txt1').css('opacity', '0');
        $('#unlock .txtBox .txt2').css('opacity', '0');
        $('#unlock .txtBox .txt3').css('opacity', '0');
        $('#unlock .txtBox .txt4').css('opacity', '0');
        $('#unlock .txtBox .txt6').css('opacity', '1');
      }
      
      $('#cut').css("height", `${screenHeight * 3}px`);
      $('#cut .sticky_container').css("height", `${screenHeight * 1.2}px`);
      const cutTop = $('#cut')[0].getBoundingClientRect().top
      const cutScrolled = - (cutTop / (screenHeight * 2));
      if ($(window).width() < 769) {
         $('#cut').css("height", `${screenHeight * 1}px`);
      }
      if (cutScrolled > 0.3) {
        $('.fit_img').removeClass('active');
        $('.cut_img img').addClass('active');
      } else {
        $('.fit_img').addClass('active');
        $('.cut_img img').removeClass('active');
      }
      if (cutScrolled > 0.5) {
        $('.cut_txt').addClass('active');
      } else {
        $('.cut_txt').removeClass('active');
      }
    }

    // 初始化样式
    setScrollTop();
    // 将动画绑定在滚动条上
    window.addEventListener('scroll', setScrollTop);


  })
</script>

<script>
  const spinner = $(".buyProducts");
  let quantityBundle = 1;
  const max = {{ main_variant.inventory_quantity }};
  function copyTxt() {
    $('.clipboard_btn').html('{{ pageTxt.copied }}');
  }

  function quantityInput() {
    const event = window.event;
    const { value } = event.target;
    if (/^[0-9]{0,5}$/.test(value)) {
      if(value > max) {
         quantityBundle = max;
         openModal(`<p>{{ pageTxt.maxQuantity }}</p>`)
      }
      quantityBundle = value;
    };
    $('.quantity-bundle').val(quantityBundle);
  }

  quantitySet = type => {
    const quantity = Number($('.quantity-bundle').val());
    // 判断是否达到最大/最小值
    if (type === 'add' && quantity >= max) openModal(`<p>{{ pageTxt.maxQuantity }}</p>`);
    const quant = ((type === 'reduce' && quantity > 1) && (quantity - 1))
      || ((type === 'add' && quantity < max) && (quantity + 1))
      || quantity;
    // 判断是执行加还是减操作并给予变化
    quantityBundle = quant;
    if(quantityBundle <= 1) {
      $('#btn_reduce').addClass('disable');
    } else {
      $('#btn_reduce').removeClass('disable');
    };
    $('.quantity-bundle').val(quantityBundle);
  }

  $(document).ready(function(){
    var clipboard = new Clipboard('.clipboard_btn');
    $('.quantity-bundle').val(quantityBundle);
    if(quantityBundle <= 1) {
      $('#btn_reduce').addClass('disable');
    } else {
      $('#btn_reduce').removeClass('disable');
    };
  })
  function line_item(variant){
    let line_item = {};
    line_item.variant_id = variant.id;
    line_item.quantity = quantityBundle;
    return line_item;
  }
  function bundleBuynow() {
    spinner.addClass('active');
    let line_items = [];
    line_items.push(line_item({{ main_variant | json }}));
    line_items.push(line_item({{ sub_variant | json }}));

    Shopify.theme.bundleBuynow({
      authorization: "{{ authorization }}",
      checkout: {
        line_items: line_items,
        presentment_currency: "{{ shop.currency }}",
        is_upstream_button: true,
        page_type: "product",
        secret: true,
        wallet_name: "Checkout"
      },
      onSuccess: () => {},
      onError: err => {
        $.dialog({ title: err.message, content: err.description, type: 'red' });
        spinner.removeClass('active');
      }
    });
  }
</script>

