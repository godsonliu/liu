<script>
  $(function() {
    const custom_variants = JSON.parse('{{ custom_variants | json }}');
    if (custom_variants) {
      let sku = "";
      const Moblie_text = JSON.parse('{{ Moblie_text | json }}');
      {%- for variant in product.variants -%}
        {% if forloop.first == true %}
          sku = "{{ variant.sku }}"
        {% endif %}
      {% endfor %}
      const dom = custom_variants.sku.map((v, i) => {
        const act = sku === v ? 'current' : '';
        return `<li class="color ${act}" data-sku="${v}" onclick="location.href='/products/${v}#template-product-t8520'">
          <a href="/products/${v}#template-product-x8">
            <div class="dot" style="background: ${custom_variants.background[i]}"></div>
          </a>
          <span class="txt"> ${custom_variants.color_txt[i]}</span>
        </li>`;
      }).join('');
      if (custom_variants.sku.indexOf(sku) > -1) {
        $('.custom_variants').html(`<ul class="options">${dom}</ul>`);
        $('.custom_variants').removeClass('swap--visible');
      }
    }
    $(".lb_2").slick({
      dots: true,
      // autoplay:true,
      infinite: true,
      slidesToShow:1,
      prevArrow: $('.slick-prev'),
      nextArrow: $('.slick-next'),
    });
    $(".y_1").click (function(){
      $(".y_1").css('display','none');
      $(".y_2").css('display','block');
      $(".y_3").css('display','block');
      $(".y_1_s").css('display','block');
      $(".y_2_s").css('display','none');
      $(".y_3_s").css('display','none');
      $(".qh_content_1").css('display','block');
      $(".qh_content_2").css('display','none');
      $(".qh_content_3").css('display','none');
    });
    $(".y_2").click (function(){
      $(".y_1").css('display','block');
      $(".y_2").css('display','none');
      $(".y_3").css('display','block');
      $(".y_1_s").css('display','none');
      $(".y_2_s").css('display','block');
      $(".y_3_s").css('display','none');
      $(".qh_content_1").css('display','none');
      $(".qh_content_2").css('display','block');
      $(".qh_content_3").css('display','none');
    });
    $(".y_3").click (function(){
      $(".y_1").css('display','block');
      $(".y_2").css('display','block');
      $(".y_3").css('display','none');
      $(".y_1_s").css('display','none');
      $(".y_2_s").css('display','none');
      $(".y_3_s").css('display','block');
      $(".qh_content_1").css('display','none');
      $(".qh_content_2").css('display','none');
      $(".qh_content_3").css('display','block');
    });
    const T8520_main = {
      init_num: (v) => {
        if (v > 0.9) return 1;
        if (v < 0.1) return 0;
        return v;
      },
      getRandom: (n, m) => {
        var num = Math.floor(Math.random() * (m - n + 1) + n);
        return num;
      },
    }

    const width = window.innerWidth;
    const viewPortHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
    let h = 600;
    if (width > 769) {
      h = viewPortHeight - 92;    
    } else {
      h = viewPortHeight - 60;
    }
    if (width > 769) {
      $('.T8520_main .content_3').height(h * 4.5 + 'px');
    } else {
      $('.T8520_main .content_3').height(h * 4 + 'px');
    }
    
    const s10_cont3 = h ;
    const s10_cont4 = h * 2;
    const s10_cont6 = h * 1.8;
    const s10_cont5 = h * 3;
    
    $(window).scroll(function(){
       
      const top = $(window).scrollTop();
      
      if($('.dh_fd').length > 0){
        const dh_fd = $('.dh_fd').offset().top;
        const dh_div = $('.dh_fd').height();
        const skia = top - dh_div; 
        if (skia > 0 ) {
          $(`.dh_fd`).css('position', `fixed`);
          $(`.dh_fd`).css('top', $(window).width() < 769 ? '78px' : '111px');
        } else {
          $(`.dh_fd`).css('position', `relative`);
          $(`.dh_fd`).css('top', `0px`);
        }
      }
      
      const sticky1_position = $('.content_3').offset().top;
      const ski1 = top - sticky1_position;
      
      if (ski1 > 0 && ski1 < s10_cont3) {
        const opacity_1 = ski1 / h;
        $(`.p_z`).css('transform', `translateY(-${300 * opacity_1}px)`);
        $(`.p_z`).css('opacity', 1 - opacity_1*10);
      }
      if (ski1 > h * 0.2  && ski1 <  s10_cont4) {
        const opacity = (ski1 - h * 0.2) / (h * 1.96 );
        if (width > 769) {
          $(`.dh_1`).css('transform', `scale(${1 - ( 1 * opacity)})`);
          if (width >= 1600){
            $(`.dh_1`).css('top', `-${1250 * opacity}px`);
          }else if(width<1600 && width>=1200){
            $(`.dh_1`).css('top', `-${1350 * opacity}px`);
          }else if(width<1200 && width>=1024){
            $(`.dh_1`).css('top', `-${1000 * opacity}px`);
          }else if(width<1024){
            $(`.dh_1`).css('top', `-${900 * opacity}px`);
          }
        } else {
          $(`.dh_1`).css('transform', `scale(${1 - ( 1 * opacity)})`);
          $(`.dh_1`).css('top', `-${100 * opacity}px`);
          
        }
      }
      if (ski1 > s10_cont4){
        const opacity_2 = (ski1 - 2*h) / h;
        //$(`.dh_1`).css('opacity', 1 -(opacity_2));
        //$(`.dh_tu`).css('opacity', opacity_2);
        //$(`.dh_tu img`).css('transform', `scale(${0.8 + ( 0.2 * opacity_2)})`);
        $(".dh_1").addClass("active_1");
        $(".dh_tu .dh_img").addClass("active_2");
      }else{
        $(".dh_1").removeClass("active_1");
        $(".dh_tu .dh_img").removeClass("active_2");
      }
    })

    referralTrack('{{ template.suffix }}', '{{ page.handle }}', 'page');
  })
</script>

