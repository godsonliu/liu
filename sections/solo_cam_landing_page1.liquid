<script type="text/javascript">		

    $(document).ready(function() {
        var roundaboutUl = $("#featured-area ul");
        var roundaboutLi = roundaboutUl.children("li");
        var pannels = $('#content .a1');
        roundaboutUl.roundabout({
            easing: 'easeOutInCirc',
            duration: 600,
            autoplay:true,
            autoplayDuration: 3000,
            autoplayPauseOnHover: true,
            enableDrag: true,
            autoplayCallback:function() { 
                var index = roundaboutUl.find(".roundabout-in-focus").index(); 
                
                pannels.eq(index).addClass("show").siblings().removeClass("show"); 
            }
        });
        roundaboutLi.click(function() {
            var index = roundaboutLi.index($(this));
            pannelChange(index);
        });
        function pannelChange(number) {
            var pannels = $('#content .a1');
            pannels.removeClass('show');
            pannels.eq(number).addClass('show');
        }
    });
    $('.dh_a').click(function() {
        for (var i = 0; i < $('.dh_a').length; i++) {
            $('.dh_a').eq(i).css({
                'background': '',
            });
        }
        $(this).css({
            'background': '#474747',
        }).siblings().removeClass('active');
    });
    
</script> 
<script>
    $(function() {
        $(".lb_all").slick({
        dots: true,
        //fade:true,
        speed:3000,
        pauseOnHover:false,
        arrows:false,
        autoplay:true,
        infinite: true,
        slidesToShow:1,
        });
        
      const Solo_cam_page1 = {
       
        init_num: (v) => {
          if (v > 0.9) return 1;
          if (v < 0.1) return 0;
          return v;
        },
        getRandom: (n, m) => {
          var num = Math.floor(Math.random() * (m - n + 1) + n);
          return num;
        },
      }
      
      

      const width = window.innerWidth;
      const viewPortHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
      let h = 600;
      if (width > 769) {
        h = viewPortHeight - 92;    
      } else {
        h = viewPortHeight - 60;
      }
      if (width > 769) {
        $('.Solo_cam_page1 .banner_7').height(h * 3 + 'px');
        $('.Solo_cam_page1 .banner_8').height(h * 3 + 'px');
      } else {
        $('.Solo_cam_page1 .banner_7').height(h * 2 + 'px');
        $('.Solo_cam_page1 .banner_8').height(h * 3 + 'px');
      }
      const s10_cont1 = h ;
      
      $(window).scroll(function(){
         
        const top = $(window).scrollTop();
        const sticky7_position = $('.banner_7').offset().top;
        const sticky8_position = $('.banner_8').offset().top;
        const sticky10_position = $('.banner_10').offset().top;
        const sticky12_position = $('.banner_12').offset().top;

        const ski7 = top - sticky7_position;
        const ski8 = top - sticky8_position;
        const ski10 = top - sticky10_position;
        const ski12 = top - sticky12_position;

        if($('.dh_fd').length > 0){
            const dh_fd = $('.dh_fd').offset().top;
            const dh_div = $('.dh_fd').height();
            const skia = top - dh_div; 
            if (skia > 0 ) {
            $(`.dh_fd`).css('position', `fixed`);
            $(`.dh_fd`).css('top', $(window).width() < 769 ? '78px' : '99px');
            } else {
            $(`.dh_fd`).css('position', `relative`);
            $(`.dh_fd`).css('top', `0px`);
            }
        }
        if (ski7 > h){
            
            $(".banner_7 .banner_7_btm").addClass("active");
        }else{
            
            $(".banner_7 .banner_7_btm").removeClass("active");
        }
        if(ski7 <0){
            $(`.banner_7_fd`).css('transform', 'translateX(0%)');
            $(`.banner_7_fd img`).css('transform', `scale(1)`);  
        }
        if (ski7 > 0 && ski7 < h){
            const opacity = ski7/ s10_cont1;
            $(`.banner_7_fd`).css('transform', `translateX(-${100 * opacity}%)`);
            $(`.banner_7_fd img`).css('transform', `scale(${1 + 20 * opacity})`);
        }
        if (ski8 < 0){
            if(width > 769){
                $(`.banner_8 .gd_btm`).css('width', `60vw`);
                $(`.banner_8 .banner_8_p3`).css('left', `61.5vw`);
            }else{
                $(`.banner_8 .gd_btm`).css('width', `90vw`);
            }
            
        }
        if (ski8 > 0 && ski8 < h){
            const opacity = ski8/ s10_cont1;
            if(width > 769){
                $(`.banner_8 .gd_btm`).css('width', `${60 - 60 * opacity}vw`);
                
            }else{
                $(`.banner_8 .gd_btm`).css('width', `${90 - 90 * opacity}vw`);
            }
            
        }
        if(ski8 > h){
            $(`.banner_8 .gd_btm`).css('width', `0px`);
        }
        if (width > 769){
            
            if(ski8 > 0 && ski8 < h){
                const opacity = ski8/ s10_cont1;
                $(`.banner_8 .banner_8_p3`).css('left', `${61.5 - 60 * opacity}vw`);
                
            }
            if(ski8 > h){
                $(`.banner_8 .banner_8_p3`).css('left', '1.5vw');
            }
        }
        if (width > 769){
            
            if(ski8 > 0.7 * h){
                $(`.banner_8 .banner_8_p2`).addClass("active");    
            }else{
                $(`.banner_8 .banner_8_p2`).removeClass("active");
            }
        }
        if (ski10 > -0.6*h){
            
            $(".banner_10_shou").addClass("active");
        }else{
            
            $(".banner_10_shou").removeClass("active");
        }
        if (ski12 > -0.4*h){
            $(`.banner_12 .banner_12_battery_font`).addClass("active");
            
        }else{
            $(`.banner_12 .banner_12_battery_font`).removeClass("active");
        }
      })
    })
  </script>