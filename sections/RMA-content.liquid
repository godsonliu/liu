                    <span class="placeholder">{{ lang.product_name }}</span>
                    <input autocomplete="nope" type="text" maxlength="200" class="ex_name" name="name" value="" onChange="changeProd('ex')">
                  </p>
                  <p class="input-normal col">
                    <span class="placeholder">{{ lang.model }}</span>
                    <input autocomplete="nope" type="text" maxlength="200" class="ex_model" name="sku" value="" onChange="changeProd('ex')">
                  </p>
                </div>
                <p class="input-normal">
                  <span class="placeholder">{{ lang.SN }}</span>
                  <input autocomplete="nope" type="text" maxlength="200" class="ex_series_no" name="series_no" value="" onChange="changeProd('ex')">
                </p>
              </div>
            </div>

            <div class="details content">
              <h4>
                {{ lang.form_details }}
                <i class="requiredFlag">*</i>
                <span class="remark">{{ lang.details_tips }}</span>
              </h4>
              <p class="textarea-normal">
                <textarea class="ex_other" name="other" value="" autocomplete="nope" onChange="valueChange('ex')"></textarea>
              </p>
            </div>

            <div class="Spin addressSpin">
              <div class="content cnt">
                <div class="address content">
                  <h4>
                    {{ lang.form_address }}
                    <span class="remark">{{ lang.address_tips }}</span>
                  </h4>
                  <div class="addressInfo">
                    <div class="col2">
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.address1 }}</span>
                          <input type="text" maxlength="200" class="ex_address1" name="address1" value="" onChange="valueChange('ex')">
                        </p>
                      </div>
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.address2 }}</span>
                          <input type="text" maxlength="200" class="ex_address2" name="address2" value="" onChange="valueChange('ex')">
                        </p>
                      </div>
                    </div>
                    <div class="col2">
                      <div class="col">
                        <p class="select-normal">
                          <span class="placeholder">{{ lang.country }}</span>
                          <select class="ex_country_id" name="country_id" onChange="countryChange('ex')" value="">
                            {{ lang.rmaCountries }}
                          </select>
                        </p>
                      </div>
                      <div class="col">
                        <p class="select-normal">
                          <span class="placeholder">{{ lang.state }}</span>
                          <select class="ex_state_id" name="state_id" onChange="valueChange('ex')"></select>
                        </p>
                      </div>
                    </div>
                    <div class="col2">
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.city }}</span>
                          <input type="text" maxlength="200" class="ex_city" name="city" value="" onChange="valueChange('ex')">
                        </p>
                      </div>
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.zip_code }}</span>
                          <input type="text" maxlength="200" class="ex_zip_code" name="zip_code" value="" onChange="valueChange('ex')">
                        </p>
                      </div>
                    </div>
                    <p class="input-normal">
                      <span class="placeholder">{{ lang.phone_number }}</span>
                      <input type="text" maxlength="200" class="ex_phone" name="phone" value="" onChange="valueChange('ex')">
                    </p>
                  </div>
                </div>
              </div>
              <div class="mask"></div>
              <div class="spinner">
                <i class="iconfont">&#xe6c0;</i>
                <div class="focus"><i class="iconfont">&#xe6c0;</i></div>
              </div>
            </div>

            <div class="Spin captchaSpin">
              <div class="content cnt">
                <div class="captcha-box con">
                  <span class="input-normal captcha">
                    <input autocomplete="nope" class="ex_captcha" name="captcha" placeholder="{{ lang.placeholder }}" value="" onChange="valueChange('ex')">
                    <a class="ex_captcha_img" role="button" tabindex="0" onClick="{getCaptchaCode('ex')}"></a>
                  </span>
                  <span class="btns">
                    <a class="button-normal reset" onClick="resetForm('ex')">{{ lang.reset }}</a>
                    <button type="submit" class="button-normal submit">{{ lang.submit }}</button>
                  </span>
                </div>
              </div>
              <div class="mask"></div>
              <div class="spinner">
                <i class="iconfont">&#xe6c0;</i>
                <div class="focus"><i class="iconfont">&#xe6c0;</i></div>
              </div>
            </div>

          </form>

        </div>
        <div class="mask formMask"></div>
        <div class="spinner formSpinner">
          <i class="iconfont">&#xe6c0;</i>
          <div class="focus"><i class="iconfont">&#xe6c0;</i></div>
        </div>
      </div>
    </div>

  </div>

  <div class="notes">{{ lang.notes_txt2 }}</div>
</div>

