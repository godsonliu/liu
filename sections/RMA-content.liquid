{% assign lang = section.settings %}

<div class="_page" style="display: none">{{ lang.jsText }}</div>
<div class="RMA">
  <div id="myModal" class="mymodal">
    <div class="dialogModal">
      <div class="modalContent">
        <button
          id="modal_close" type="button" class="modalClose"
          onclick="closeModal()"
        >
          <i class="iconfont">&#xe723;</i>
        </button>
        <div id="modal_con"></div>
      </div>
    </div>
  </div>
  <div class="notes">{{ lang.notes_txt1 }}</div>

  <div class="infos">
    <nav>
      <div class="scrollnav">
        {% if lang.show_store %}<h5 id="official">{{ lang.store }}</h5>{% endif %}
        {% if lang.show_amazon %}<h5 id="amazon">{{ lang.amazon }}</h5>{% endif %}
        {% if lang.show_ebay %}<h5 id="eBay">{{ lang.eBay }}</h5>{% endif %}
        {% if lang.show_walmart %}<h5 id="Walmart">{{ lang.walmart }}</h5>{% endif %}
        {% if lang.show_bestbuy %}<h5 id="BestBuy">{{ lang.bestbuy }}</h5>{% endif %}
        {% if lang.show_target %}<h5 id="Target">{{ lang.target }}</h5>{% endif %}
        {% if lang.show_others %}<h5 id="Others">{{ lang.others }}</h5>{% endif %}
      </div>
    </nav>

    <div class="forms">
      <div class="Spin formSpin">
        <div class="content cnt formContent">

          <form id="official_form" class="official_form" onSubmit="rmaConfirm('official')">
            <div class="info content">
              <h4>{{ lang.form_info }}</h4>
              <div class="col2">
                <p class="input-normal col">
                  <span class="placeholder">{{ lang.email }}<i class="requiredFlag">*</i></span>
                  <input
                    type="text" autocomplete="nope" maxlength="200"
                    name="email" value="" onChange="valueChange('official')"
                  />
                </p>
                <p class="input-normal col">
                  <span class="placeholder">{{ lang.confirm_email }}<i class="requiredFlag">*</i></span>
                  <input
                    type="text" autocomplete="nope" maxlength="200"
                    name="email_confirm" value="" onChange="valueChange('official')"
                  />
                </p>
              </div>
              <div class="col2">
                <p class="input-normal col">
                  <span class="placeholder">{{ lang.first_name }}<i class="requiredFlag">*</i></span>
                  <input
                    type="text" autocomplete="nope" maxlength="200"
                    name="first_name" value="" onChange="{valueChange('official')}"
                  />
                </p>
                <p class="input-normal col">
                  <span class="placeholder">{{ lang.last_name }}<i class="requiredFlag">*</i></span>
                  <input
                    type="text" autocomplete="nope" maxlength="200"
                    name="last_name" value="" onChange="{valueChange('official')}"
                  />
                </p>
              </div>
            </div>
            
            <div class="order content">
              <h4>{{ lang.form_order }}</h4>
              <div class="Spin orderSpin">
                <div class="content cnt">
                  <div class="input-normal">
                    <span class="placeholder">{{ lang.order_id }}<i class="requiredFlag">*</i></span>
                    <div class="col2">
                      <p class="col">
                        <input type="text" autocomplete="nope" maxlength="200" name="order_id" value="" onChange="{valueChange('official')}">
                      </p>
                      <p class="col">
                        <button type="button" class="button-normal submit" onClick="verifyOrder('official')">{{ lang.verify_order }}</button>
                      </p>
                    </div>
                    <a class="tip" onclick="openModal('<p style=\'width: 300px;\'>For guest users, enter the {{ lang.order_id }} listed in the confirmation email you received. For registered customers, simply sign in to sync your order history automatically.</p>')">
                      {{ lang.how_find_order }}
                    </a>
                  </div>
                </div>
                <div class="mask"></div>
                <div class="spinner">
                  <i class="iconfont">&#xe6c0;</i>
                  <div class="focus"><i class="iconfont">&#xe6c0;</i></div>
                </div>
              </div>
                
              <div class="prods_info"></div>
            </div>

            <div class="Spin addressSpin">
              <div class="content cnt">
                <div class="address content">
                  <h4>
                    {{ lang.form_address }}
                    <span class="remark">{{ lang.address_tips }}</span>
                  </h4>
                  <div class="addressInfo">
                    <div class="col2">
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.address1 }}</span>
                          <input type="text" maxlength="200" name="address1" value="" onChange="valueChange('official')">
                        </p>
                      </div>
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.address2 }}</span>
                          <input type="text" maxlength="200" name="address2" value="" onChange="valueChange('official')">
                        </p>
                      </div>
                    </div>
                    <div class="col2">
                      <div class="col">
                        <p class="select-normal">
                          <span class="placeholder">{{ lang.country }}</span>
                          <select name="country_id" onChange="countryChange('official')" value="">
                            {{ lang.rmaCountries }}
                          </select>
                        </p>
                      </div>
                      <div class="col">
                        <p class="select-normal">
                          <span class="placeholder">{{ lang.state }}</span>
                          <select class="official_state_id" name="state_id" onChange="valueChange('official')"></select>
                        </p>
                      </div>
                    </div>
                    <div class="col2">
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.city }}</span>
                          <input type="text" maxlength="200" name="city" value="" onChange="valueChange('official')">
                        </p>
                      </div>
                      <div class="col">
                        <p class="input-normal">
                          <span class="placeholder">{{ lang.zip_code }}</span>
                          <input type="text" maxlength="200" name="zip_code" value="" onChange="{valueChange('official')}">
                        </p>
                      </div>
                    </div>
                    <p class="input-normal">
                      <span class="placeholder">{{ lang.phone_number }}</span>
                      <input type="text" maxlength="20" name="phone" value="" onChange="{valueChange('official')}">
                    </p>
                  </div>
                </div>
              </div>
              <div class="mask"></div>
              <div class="spinner">
                <i class="iconfont">&#xe696;</i>
                <div class="focus"><i class="iconfont">&#xe696;</i></div>
              </div>
            </div>

            <div class="details content">
              <h4>{{ lang.form_details }}<i class="requiredFlag">*</i></h4>
              <p class="textarea-normal">
                <textarea name="other" autocomplete="nope" onChange="valueChange('official')"></textarea>
              </p>
            </div>

            <div class="Spin attachSpin">
              <div class="content cnt">
                <div class="upload con">
                  <h4>{{ lang.upload }}</h4>
                  <div class="images">
                    {% comment %} item {% endcomment %}
                    <i class="upBox">
                      <a class="iconfont item">&#xe727;</a>
                      <input type="file" accept="image/*,application/pdf" multiple="multiple"
                        onChange="uploadAttach('official')" value
                      />
                    </i>
                  </div>
                </div>
              </div>
              <div class="mask"></div>
              <div class="spinner">
                <i class="iconfont">&#xe6c0;</i>
                <div class="focus"><i class="iconfont">&#xe6c0;</i></div>
              </div>
            </div>

            <div class="Spin captchaSpin">
              <div class="content cnt">
                <div class="captcha con">
                  <div class="captcha-box">
                    <span class="input-normal captcha">
                      <input autocomplete="nope" placeholder="{{ lang.placeholder }}" name="captcha" value="" onChange="valueChange('official')">
                      <a class="official_captcha_img" role="button" tabindex="0" onClick="{getCaptchaCode('official')}"></a>
                    </span>
                    <span class="btns">
                      <a class="button-normal reset" onClick="resetForm('official')">{{ lang.reset }}</a>
                      <button type="submit" class="button-normal submit">{{ lang.submit }}</button>
                    </span>
                  </div>
                </div>
              </div>
              <div class="mask"></div>
              <div class="spinner">
                <i class="iconfont">&#xe6c0;</i>
                <div class="focus"><i class="iconfont">&#xe6c0;</i></div>
              </div>
            </div>
            
