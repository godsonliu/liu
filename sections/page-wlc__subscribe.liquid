    top: 0;
    right: 0;
    padding: 0 3vw;
    height: 2.92vw; 
    min-height: 49px;
    outline: none;
    display: flex;
    justify-content: center;
    align-items: center;
    border: none;
    cursor: pointer;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
