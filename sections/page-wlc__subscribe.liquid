  #{{section_id}} .modal-wrap {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    z-index: 9999;
  }
  #{{section_id}} .modal {
    position: absolute;
    width: 37.5vw;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    padding: 4.16vw 4.16vw 3.12vw;
    background: #fff;
    border-radius: 20px;
    text-align: center;
  }
  #{{section_id}} .modal-close {
    position: absolute;
    top: 1vw;
    right: 1vw;
