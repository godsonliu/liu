{% assign section_id = 'shopify-section-' | append: section.id %}
<style>
#{{ section_id }} .page_weekend_deal_icon {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #999999;
  padding: 20px;
}
#{{ section_id }} .page_weekend_deal_icon_item {
  display: flex;
  align-items: center;
  margin-right: 30px;
}
#{{ section_id }} .page_weekend_deal_icon_item:last-child {
  margin-right: 0;
}
#{{ section_id }} .page_weekend_deal_icon_item svg {
  width: 40px;
  height: 40px;
}
#{{ section_id }} .page_weekend_deal_icon_txt {
  font-size: 16px;
  color: #FFFFFF;
  margin-left: 10px;
}

@media(max-width: 769px) {
  #{{ section_id }} .page_weekend_deal_icon {
    background: #F5F5F7;
    flex-direction: column;
  }
  #{{ section_id }} .page_weekend_deal_icon_item {
    width: 80%;
    height: 50px;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0 auto 20px auto !important;
    border-radius: 10px;
    background: #999;
  }
  #{{ section_id }} .page_weekend_deal_icon_txt {
    flex: 0.8;
    font-size: 15px;
  }
}
</style>

