#shopify-section-Hallowmas-content {
  img {
    vertical-align: top;
    margin-top: -5px;

    @media screen and(max-width:768px) {
      margin-top: -3px;
    }
  }

  .banner {
    position: relative;

    video {
      width: 100%;
    }

    .btn {
      cursor: pointer;
      position: absolute;
      top: 19.3vw;
      right: 24.7vw;
      height: 3vw;
      width: 3vw;
      border-radius: 50%;

      @media screen and(max-width:768px) {
        width: 100%;
        height: 100%;
        right: 0;
        top: 0;
      }
    }

    .banner_text {
      display: none;
      left: 3vw;
      bottom: 9vw;
      width: 30vw;
      font-size: 2.5vw;
      position: absolute;
      color: #f3822b;
      text-align: center;
    }

    .banner_box {
      position: absolute;
      bottom: 0;
      left: 0;
    }


  }

  .halloBg,
  .sharemob {
    width: 100%;
    vertical-align: bottom;
  }

  .share {
    position: relative;

    .share_products {
      z-index: 3;
      position: absolute;
      top: -3vw;
      width: 70vw;
      left: 50%;
      transform: translateX(-50%);
      text-align: center;
      max-width: 1200px;
      text-align: center;
      color: #fff;

      @media screen and(max-width:768px) {
        top: 0;
      }

      @media screen and(min-width:2560px) {
        top: 5vw;
      }

      h1 {
        margin-top: 20px;
        color: #f3822b;
        text-align: center;
        font-size: 40px;

        @media screen and(max-width:1300px) {
          margin-top: 10px;
          font-size: 3rem;
        }

        @media screen and(min-width:2560px) {
          font-size: 60px;
        }

        @media screen and(max-width:768px) {
          font-size: 4vw;
        }
      }

      p {
        margin: 10px auto 0;
        width: 70%;
        text-align: center;
        font-size: 26px;

        @media screen and(max-width:1300px) {
          margin: 0 auto;
          width: 90%;
          font-size: 1.5rem;
        }

        @media screen and(max-width:768px) {
          font-size: 1rem;
          width: 90%;
        }

        @media screen and(min-width:2560px) {
          font-size: 40px;
        }

        .line {
          cursor: pointer;
          text-decoration: underline;
        }
      }

      img {
        z-index: -1;
        position: absolute;
        top: 2vw;
        left: 50%;
        transform: translateX(-50%);
        max-width: 1200px;
        width: 63vw;

        @media screen and(max-width:768px) {
          width: 70vw;
          top: 10vw;
        }
      }
    }

    .sharepc {
      width: 100%;
      vertical-align: bottom;
    }

    .ktv {
      cursor: pointer;
      width: 50vw;
      z-index: 1;
      position: absolute;
      top: 100%;
      left: 50%;
      transform: translate(-50%, -50%);
      max-width: 1200px;

      iframe {
        width: 100%;
        height: 26vw;
      }

      @media screen and(max-width:1300px) {
        top: 90%;
      }

      @media screen and(max-width:768px) {
        width: 60vw;
        height: 100vw;
        overflow: hidden;
        top: 90%;

        iframe {
          width: 100%;
          height: 100vw;
          max-width: none;
        }
      }

    }

    .share_text {
      bottom: -28vw;
      max-width: 1200px;
      margin: auto;
      left: 50%;
      width: 70vw;
      text-align: center;
      transform: translateX(-50%);
      position: absolute;
      font-size: 1.7vw;
      color: #f0f0f0;
      line-height: 3vw;
      z-index: 1;

      @media screen and(min-width:1921px) {
        font-size: 40px;
      }

      @media screen and(max-width:768px) {
        bottom: -67vw;
        font-size: 1.1rem;
        line-height: 1.5rem;
      }

      img {
        width: 7vw;
        margin: 0;
        z-index: -1;

        @media screen and(max-width:768px) {
          width: 12vw;
          margin-top: -1vw;
        }

        @media screen and(max-width:530px) {
          width: 12vw;
          margin: 0;
        }
      }
    }

    .share_btn {
      z-index: 1;
      text-align: center;
      width: 30vw;
      position: absolute;
      bottom: -20vw;
      left: 50%;
      transform: translateX(-50%);
      font-size: 2vw;

      @media screen and(max-width:768px) {
        width: 70vw;
        bottom: -44vw;
        z-index: 1;
      }
    }

  }

  .watchBox {
    position: relative;
    overflow: hidden;

    h1 {
      position: absolute;
      color: #f3822b;
      top: 3vw;
      font-size: 4vw;
      max-width: 1200px;
      width: 70vw;
      text-align: center;
      left: 50%;
      transform: translateX(-50%);

      @media screen and(max-width:768px) {
        font-size: 6vw;
      }
    }

    video {
      position: absolute;
    }
  }

  .products {
    position: relative;

    .products_text_bottom {
      bottom: 21vw;
      left: 50%;
      color: #fff;
      width: 100%;
      margin-top: 10vw;

      @media screen and(max-width:768px) {
        margin-top: 20vw;
      }

      h3 {
        color: #f3822b;
        font-size: 40px;

        @media screen and(max-width:768px) {
          font-size: 5vw;
          width: 90%;
          margin: 0 auto 5vw;
        }
      }

      p {
        font-size: 1vw;
        margin: auto;
        text-align: justify;

        @media screen and(max-width:768px) {
          width: 90%;
          font-size: 1rem;
          text-align: justify;
          margin-bottom: 1vw;
        }
      }
    }

    h1 {
      margin: 0 auto 2vw;
      max-width: 1200px;
      color: #f3822b;
      text-align: center;
      font-size: 4vw;
      padding: 5vw 0 0vw;
      position: absolute;
      top: 27vw;
      left: 50%;
      transform: translateX(-50%);

      @media screen and(max-width:768px) {
        width: 100vw;
        padding: 7vw;
        font-size: 5vw;
        top: 72vw;
      }
    }

    .products_bg {
      width: 100%;
      z-index: -1;
    }

    .products_content {
      margin: auto;
      max-width: 1200px;
      width: 70vw;
      position: absolute;
      left: 50%;
      transform: translateX(-50%);
      top: 42vw;

      @media screen and(max-width:768px) {
        top: 90vw;
        padding: 0 4vw;
        width: 100vw;

      }
    }

    .products_text {
      position: relative;
      max-width: 1200px;
      width: 100%;
      margin: auto;
      color: #f0f0f0;

      @media screen and(max-width:1919px) {
        min-height: 30vw;
      }

      @media screen and(min-width:1920px) {
        min-height: 460px;
      }

      img {
        width: 100%;
        margin-top: 1vw;

        @media screen and(max-width:768px) {
          margin-top: 4vw;
          width: 100%;
          top: 0;
          left: 0;
          z-index: -1;
        }
      }

      h2 {
        color: #f0f0f0;
        font-size: 2.5vw;
        font-weight: bold;

        @media screen and(min-width:1921px) {
          font-size: 40px;
        }

        @media screen and(max-width:768px) {
          font-size: 16px;
        }
      }

      .products_content_text {
        @media screen and (width:768px) {
          position: absolute;
          left: 5vw;
          top: 17vw;
          width: 35vw;
        }

        @media screen and (min-width:769px) {
          position: absolute;
          left: 5vw;
          top: 7vw;
          width: 35vw;
        }

        @media screen and (width:1920px) {
          position: absolute;
          left: 5vw;
          top: 7vw;
        }

        @media screen and (min-width:1921px) {
          position: absolute;
          left: 100px;
          top: 120px;
        }

        @media screen and(max-width:767px) {
          position: absolute;
          left: 5vw;
          top: 6vw;
          width: 48vw;

        }

        .products_btn {
          cursor: pointer;
          display: inline-block;
          padding: .5vw 1vw;
          background-color: #f3822b;
          height: 2vw;
          font-size: 1vw;
          line-height: 1vw;
          text-align: center;
          border-radius: 1.5vw;
          margin-top: 4vw;

          &:hover {
            color: #f3822b;
            background-color: #fff;
          }

          @media screen and(min-width:1921px) {
            margin-top: 70px;
          }

          @media screen and(max-width:768px) {
            font-size: 1rem;
            height: 2rem;
            line-height: 1.7rem;
            padding: .5vw 2vw;
            border-radius: 1rem;
            margin-top: 2vw;
          }
        }
      }

      p {
        font-size: 1vw;

        @media screen and(min-width:1921px) {
          font-size: 20px;
        }

        @media screen and(max-width:768px) {
          font-size: 12px;
        }
      }

      .products_bottom {
        margin-top: 3.5vw;

        @media screen and(min-width:1921px) {
          margin-top: 70px;
        }

        @media screen and(width:1920px) {
          margin-top: 2vw;
        }

        @media screen and(max-width:768px) {
          font-size: 1rem;
          margin-top: 3vw;
          width: 80%;
        }

      }

      @media screen and(max-width:768px) {
        &:nth-child(2) {
          .products_content_text {
            top: 13vw;
          }
        }
      }
    }
  }
}

video {
  vertical-align: top;
}

.video_one,
.video_one_mob {
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}

a {
  cursor: pointer;
}

@font-face {
  font-family: 'IndieFlower';
  font-weight: bold;
  font-style: normal;
  src: url('{{ "IndieFlower.woff2" | asset_url }}') format('woff2'),
    url('{{ "IndieFlower.woff" | asset_url }}') format('woff'),
    url('{{ "IndieFlower.ttf" | asset_url }}') format('truetype'),
    url('{{ "IndieFlower.svg" | asset_url }}') format('svg');
  font-display: swap;
}

.banneralext {
  z-index: 3;
  display: none;
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, .7);

  strong {
    font-size: 31px;

    @media screen and(max-width:768px) {
      font-size: 1.5rem;
    }
  }

  .banneralext_content {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 80vw;
    max-width: 1100px;
    border-radius: 1vw;
    color: #fff;
    display: flex;
    justify-content: center;

    @media screen and(min-width:1500px) {
      max-width: 1400px;
    }

    p {
      width: 70%;
    }

    @media screen and(max-width:768px) {
      width: 90vw;
      top: 50vw;
      transform: translateX(-50%);

      p {
        width: 60%;
      }
    }

    .banneralext_content_bg {
      margin: auto;
    }

    .banneralext_content_text {
      margin-top: 3vw;
      font-size: 26px;
      text-align: center;

      @media screen and(min-width:1921px) {
        margin-top: 29px;
      }

      @media screen and(max-width:1300px) {
        font-size: 20px;
      }

      @media screen and(max-width:768px) {
        font-size: 1rem;
        margin-top: 7vw;
      }
    }

    .banneralext_content_btn {
      cursor: pointer;

      .btnpath {
        width: 100%;
        height: 100%;
        color: #fff;
      }

      @media screen and(max-width:1919px) {
        margin-top: 3vw;
      }

      @media screen and(min-width:1921px) {
        margin-top: 50px;
      }

      &:hover {
        background: #9a4001;
      }

      margin-top: 4vw;
      padding: 0 20px;
      height: 50px;
      font-size: 20px;
      width: auto;
      font-family: 'IndieFlower';
      color: #fff;
      line-height: 50px;
      background: #543017;
      border-radius: 25px;
      border: 1px solid #f3822b;

      @media screen and(max-width:1300px) {
        margin-top: 1vw;
      }

      @media screen and(max-width:768px) {
        font-size: 1rem;
        padding: 0 10px;
        height: 30px;
        line-height: 30px;
        margin-top: 7vw;
      }
    }

    .content {
      padding-top: 6vw;

      @media screen and(min-width:1921px) {
        padding-top: 100px;
      }

      position: absolute;
      z-index: 1;
      top: 0;
      left: 0;
      width: 100%;
      display: flex;
      align-items: center;
      flex-direction: column;
    }

    .banneralext_content_title {
      width: 21vw;

      @media screen and(max-width:768px) {
        width: 70vw;
      }
    }

    .rules {
      cursor: pointer;
      display: flex;
      margin-top: 5vw;
      text-align: center;
      justify-content: center;
      position: absolute;
      bottom: -9vw;

      @media screen and(min-width:1600px) {
        margin-top: 150px;
        bottom: -7vw;
      }

      @media screen and(max-width:1919px) {
        margin-top: 3vw;
      }

      @media screen and(width:1920px) {
        bottom: -5vw;
      }

      @media screen and(min-width:1921px) {
        margin-top: 150px;
        bottom: -5vw;
      }

      @media screen and(max-width:768px) {
        bottom: -35vw;
      }

      .rules_left {
        font-size: 40px;
        text-decoration: underline;
      }

      p {
        width: 100%;
        font-size: 1.3vw;

        @media screen and(max-width:768px) {
          width: 80%;
          font-size: 1rem;
        }

        @media screen and(min-width:1920px) {
          font-size: 20px;
        }

        text-decoration: underline;
        margin: auto;
      }

      .rules_right {
        padding-top: 10px;
        margin-left: 20px;
        text-align: left;
        display: flex;
        flex-direction: column;
        justify-content: center;

        p {
          width: 100%;
          margin: 0;
        }

      }

      @media screen and(max-width:768px) {
        flex-direction: column;

        .rules_left {
          font-size: 1.5rem;
        }

        .rules_right {
          font-size: 1rem;
          margin: 0;

          p {
            width: 64%;
            margin: 5px auto 0;
            text-align: center;
          }
        }
      }
    }
  }

  .codeCopy {
    color: #f3822b;
    font-weight: bold;

  }

  .closeBox {
    z-index: 2;
    cursor: pointer;
    position: absolute;
    right: 10%;
    top: 10%;

    @media screen and(max-width:768px) {
      top: 5%;
      right: 7%;
    }

    @media screen and(min-width:1600px) {
      right: 13%;
    }

    @media screen and(min-width:1920px) {
      right: 15%;
    }

    i {
      font-size: 3vw;
      color: #f3822b;
    }
  }

  .banneralext_content-Box {
    position: relative;
    width: 100%;
    height: 100%;
  }

  .CopyAll {
    cursor: pointer;
    display: inline-block;
    margin-left: 10px;
    line-height: 16px;
    background: #fa7621 !important;
    padding: 0px 1vw;
    height: 25px;

    @media screen and(max-width:768px) {
      padding: 0 2vw;
      height: 1.5rem;
      line-height: 1.5rem;
    }

    border-radius: 1rem;

    .CopyBox {
      color: #fff;
      width: auto;
      font-size: 16px;

      @media screen and(max-width:768px) {
        font-size: 1rem;
      }
    }

    img {
      width: 16px;
      margin-top: 4px !important;

      @media screen and(max-width:768px) {
        margin-top: 3px !important;
        width: 1rem;
      }

      margin-left: 3px;
    }
  }
}
